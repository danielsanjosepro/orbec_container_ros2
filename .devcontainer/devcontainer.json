{
    "name": "LSY Franka",
    "privileged": true,
    "remoteUser": "ros",
    "dockerComposeFile": "./docker-compose.yaml",
    "workspaceFolder": "/home/ros/ros2_ws/src/lsy_franka",
    "service": "lsy_franka",
    "features": {
        "ghcr.io/duduribeiro/devcontainer-features/neovim:1": {
            "version": "stable"
        },
        "ghcr.io/devcontainers/features/node:1": {
            "version": "lts"
        },
        "ghcr.io/larsnieuwenhuizen/features/zellij:0": {
            "version": "0.0.4"
        }
    },
    "customizations": {
        "vscode": {
        "extensions": [
            "ms-python.python",
            "ms-iot.vscode-ros"
            ]
        }
    },
    "mounts": [
        "source=${localWorkspaceFolder},target=/home/ros/ros2_ws/src/orbec_camera_ros2,type=bind,consistency=cached",
    ],
}
