{
    "name": "Orbec Camera ROS2 Dev Container",
    "privileged": true,
    "remoteUser": "ros",
    "dockerComposeFile": "./docker-compose.yaml",
    "workspaceFolder": "/home/ros/ros2_ws/src/orbec_container_ros2",
    "service": "orbec_camera_ros2",
    "features": {
        "ghcr.io/meaningful-ooo/devcontainer-features/homebrew:2": {}
    },
    "customizations": {
        "vscode": {
        "extensions": [
            "ms-python.python",
            "ms-iot.vscode-ros"
            ]
        }
    },
    "mounts": [
        "source=${localWorkspaceFolder},target=/home/ros/ros2_ws/src/orbec_camera_ros2,type=bind,consistency=cached",
    ],
}
